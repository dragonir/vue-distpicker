{
  "name": "vue-distpicker",
  "version": "1.0.5",
  "description": "A simple Vue component for picking provinces, cities and districts of China.",
  "repository": {
    "url": "sinchang/vue-distpicker",
    "type": "git"
  },
  "main": "dist/vue-distpicker.cjs.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint .",
    "test": "tyu -- --coverage",
    "test:watch": "tyu -- --watch",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "sinchang",
    "email": "sinchangwen@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": [
      "cjs",
      "umd"
    ],
    "name": "vue-distpicker",
    "plugins": [
      "vue"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ]
  },
  "eslintIgnore": [
    "*.test.js",
    "dist",
    "coverage"
  ],
  "devDependencies": {
    "bili": "^1.3.3",
    "eslint": "^4.14.0",
    "eslint-plugin-vue": "^4.0.1",
    "poi": "^9.3.10",
    "rollup-plugin-vue": "^2.5.2",
    "tyu": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.9"
  }
}
